<div class="composetitle_container">
<h1> Compose post </h1>
</div>
<div class="post_container">
  <div id="compose_posts">
  <%= form_for @post do |f| %>
  <%= f.label :posttitle %><br>
  <%= f.text_field :posttitle %><br>
  <%= f.label :postbody %><br>
  <%= f.text_area :postbody %><br>
  <%= f.submit "Publish" %>
  <% end %>
  </div>
</div>

